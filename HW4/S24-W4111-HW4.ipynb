    "\"\"\"\n",
    "Tips:\n",
    "\n",
    "    To iterate through dataframe:\n",
    "    \n",
    "        for _, r in customer_orders_all_df.iterrows():\n",
    "            r = dict(r)\n",
    "            Access fields like r['customerName'], r['country'], ...\n",
    "            \n",
    "    \n",
    "    The orderDate and priceEach fields are stored as datetime.date and Decimal\n",
    "    objects in the dataframe. These types are not compatible with the pymongo API.\n",
    "    You can convert them to strings by calling str(r['orderDate']) and str(r['priceEach']). \n",
    "    Alternatively, you can look into the datetime.datetime and bson.decimal128.Decimal128 \n",
    "    objects, which are supported by pymongo.\n",
    "\n",
    "\"\"\"\n",
    "def createOrderContent(row):\n",
    "    '''\n",
    "    Helper function to create orderContents field in df\n",
    "    '''\n",
    "    return {\n",
    "        'productCode': row['productCode'],\n",
    "        'quantityOrdered': row['quantityOrdered'],\n",
    "        'priceEach': str(row['priceEach'])\n",
    "    }\n",
    "\n",
    "def createOrder(group):\n",
    "    '''\n",
    "    Helper function: on input of groupby on orderNumber, \n",
    "    creates an order format per group and outputs new df.\n",
    "    '''\n",
    "    order={\n",
    "        'orderNumber': int(group['orderNumber'].values[0]),\n",
    "        'orderDate' : str(group['orderDate'].values[0]),\n",
    "        'orderContents': list(group['orderContents'])\n",
    "        }\n",
    "    new_df = pandas.DataFrame()\n",
    "    new_df['order']= [order]\n",
    "    new_df['customerNumber']=group['customerNumber'].values[0]\n",
    "    new_df['customerName']=group['customerName'].values[0]\n",
    "    new_df['country']=group['country'].values[0]\n",
    "    new_df['orderNumber']=group['orderNumber'].values[0]\n",
    "    return new_df\n",
    "\n",
    "def createCustomer(group):\n",
    "    '''\n",
    "    Helper function: on input of groupby on customerNumber, \n",
    "    creates dictionary of final output format.\n",
    "    '''\n",
    "    final={\n",
    "        'customerNumber':int(group['customerNumber'].values[0]),\n",
    "        'customerName': group['customerName'].values[0],\n",
    "        'country': group['country'].values[0],\n",
    "        'orders': list(group['order'])\n",
    "    }\n",
    "    return final\n",
    "\n",
    "#sort df to ensure no groupby anomalies\n",
    "df=customer_orders_all_df.sort_values(by='customerNumber')\n",
    "\n",
    "#create orderContents\n",
    "df['orderContents']=df.apply(createOrderContent,axis=1)\n",
    "\n",
    "#create order format for each orderNumber\n",
    "df['ordering']=df['orderNumber'].copy()\n",
    "orders=df.groupby('ordering').apply(createOrder,include_groups=False)\n",
    "\n",
    "#create final format of dict for each customer\n",
    "orders['customers']=orders['customerNumber'].copy()\n",
    "result=orders.groupby('customers').apply(createCustomer,include_groups=False)\n",
    "\n",
    "#output result as list of dicts\n",
    "customer_info=list(result.values)"

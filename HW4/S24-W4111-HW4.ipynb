    "import json\n",
    "\n",
    "def prepr(doc):\n",
    "    try:\n",
    "        del doc['_id']\n",
    "    except KeyError:\n",
    "        pass\n",
    "    \n",
    "    def convert_str(d):\n",
    "        if isinstance(d, dict):\n",
    "            for k, v in d.items():\n",
    "                d[k] = convert_str(v)\n",
    "            return d\n",
    "        elif isinstance(d, list):\n",
    "            for i, v in enumerate(d):\n",
    "                d[i] = convert_str(v)\n",
    "            return d\n",
    "        else:\n",
    "            return str(d)\n",
    "                \n",
    "    convert_str(doc)\n",
    "    return json.dumps(doc, indent=2)"
   ]
